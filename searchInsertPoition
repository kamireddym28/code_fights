class Solution:
    def searchInsert(self, nums, target):
        """
        :type nums: List[int]
        :type target: int
        :rtype: int
        """
        n = len(nums)
        if n == 0 or target < nums[0]:
            return 0
        if target > nums[n-1]:
            return n
        for i in range(n):
            if nums[i] == target:
                return i
            elif nums[i] < target and nums[i+1] > target:
                nums.insert(i+1, target)
                return i+1
